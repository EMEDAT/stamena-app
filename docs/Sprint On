//         ) : (
//           <Stack.Screen name="Main" component={MainTabNavigator} />
//         )}
//       </Stack.Navigator>
//     </NavigationContainer>
//   );
// };
// ```

// Create `src/navigation/OnboardingNavigator.tsx`:
